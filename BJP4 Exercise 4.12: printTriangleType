import java.util.*;
public void printTriangleType(int a, int b, int c){
    if(a!=b&&b!=c&&a==c)
    System.out.println("isosceles");
    if(a==b&&b!=c&&a!=c)
    System.out.println("isosceles");
    if(a!=b&&b==c&&a!=c)
    System.out.println("isosceles");
    if(a==b&&b==c&&a==c)
    System.out.println("equilateral");
    if(a!=b&&b!=c&&a!=c)
        System.out.println("scalene");
}
